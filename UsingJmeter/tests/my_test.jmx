                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">var workspaceId = vars.get(&quot;workspaceId&quot;);
var projectName = &apos;Project_1&apos;;
var projectDes = &apos;ProjectDescription_One&apos;;

vars.put(&quot;projectName&quot;, projectName);
