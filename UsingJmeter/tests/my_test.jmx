              <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
                <stringProp name="scriptLanguage">javascript</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">false</stringProp>
                <stringProp name="script">var responseCode = SampleResult.getResponseCode();
var responseBody =  SampleResult.getResponseDataAsString();

if (responseCode == 201) {

	var data = JSON.parse(responseBody);
	var ProjectName = data.name;
	var expectedProjectName = vars.get(&quot;projectName&quot;);
	
	if (ProjectName == expectedProjectName) {
		AssertionResult.setFailure(false);
		SampleResult.setResponseMessage(&quot;Project has been created successfully&quot;);
		log.info(&quot;Post Request is Successful&quot;);
		} else {
		AssertionResult.setFailure(true);
		AssertionResult.setFailureMessage(&quot;Post Request is unsuccessful&quot;);
		log.info(&quot;Project Name is mismatched&quot;);
			}
	}
</stringProp>
              </JSR223Assertion>
